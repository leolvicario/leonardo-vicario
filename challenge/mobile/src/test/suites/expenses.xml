                    <include name = "categoryPercentages"/>
                </methods>
            </class>
            <class name = "demo.automation.challenge.MovementTest">
                <methods>
                    <include name = "movementEntry"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>