<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name = "Monefy - Expenses">
    <parameter name="reportName" value="Expenses_Report"/>
    <test name = "Expenses Suite">
        <classes>
            <class name = "demo.automation.challenge.ExpenseTest">
                <methods>
                    <include name = "addExpense"/>
